{
	"lerna": "2.9.0",
	"npmClient": "yarn",
	"useWorkspaces": true,
	"version": "10.0.4",
	"command": {
		"publish": {
			"allowBranch": [
				"dev",
				"canary",
				"prerelease-*"
			],
			"message": "chore(release): %v [skip-ci-version]",
			"conventionalCommits": true,
			"changelogPreset": "sprucelabs"
		}
	}
}